    "format": "biome format --write",
    "build": "rimraf ./dist/ && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "i18n": "i18next",
    "knip": "knip"
